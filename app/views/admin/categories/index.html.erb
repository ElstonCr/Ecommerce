<div style="padding: 20px;">
  <p style="color: green"><%= notice %></p>

  <h1>Categories &nbsp;&nbsp;&nbsp;<%= link_to "New category", new_admin_category_path, class:"btn btn-outline-primary"%> </h1>
  <div style="padding: 35px;" id="admin_categories">
    <% @admin_categories.each do |admin_category| %>
      <%= render admin_category %>
      <p>
        <%= link_to "Show this category", [:admin, admin_category], class:"btn btn-outline-secondary" %>
      </p>
    <% end %>
  </div>
</div>
